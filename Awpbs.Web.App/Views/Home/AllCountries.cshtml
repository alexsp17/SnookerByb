@{
    ViewBag.Title = "All Countries";
}

<div class="container text-center" style="background-color:transparent">
    <div class="row">
        <div class="text-center" style="margin-bottom:20px;margin-top:20px;">
            @foreach (var country in Awpbs.Country.ListWithoutImportance0)
            {
                <p><a href="~/@country.UrlName">@country.Name</a></p>
            }
        </div>
    </div>
</div>

<hr />

<div class="container text-center" style="background-color:transparent">
    <div class="row">
        @Html.Partial("~/Views/Home/_MobileAppsPartial.cshtml")
    </div>
</div>

